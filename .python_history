from models import db, User
from app import db, User
try:
    user_count = User.query.count()
    print(f"\nCONEXÃO BEM-SUCEDIDA!")
    print(f"O banco de dados '{db.engine.url.database}' está conectado.")
    print(f"A tabela 'user' tem {user_count} registro(s).\n")
except Exception as e:
    print(f"\nFALHA NA CONEXÃO.")
    print(f"Ocorreu um erro ao tentar acessar o banco: {e}\n")
